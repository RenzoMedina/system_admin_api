{
    "name": "renzomedina/backend",
    "description": "This project to admin system",
    "type": "project",
    "license": "none",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Routes\\": "routes/",
            "Core\\":"core/"
        },
        "files": ["functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\":"test/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit --testdox --colors=always",
        "coverage": "vendor/bin/phpunit --coverage-html html-coverage",
        "precoverage": "rm -rf html-coverage"
    }
    ,
    "authors": [
        {
            "name": "RenzoMedina",
            "email": "medinast30@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "flightphp/core": "dev-master",
        "vlucas/phpdotenv": "^5.6@dev",
        "firebase/php-jwt": "dev-main",
        "catfan/medoo": "dev-master",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3@dev"
    }
}
